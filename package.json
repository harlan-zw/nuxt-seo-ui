{
  "name": "nuxt-seo-ui",
  "type": "module",
  "version": "0.4.1",
  "packageManager": "pnpm@8.9.2",
  "description": "Nuxt UI components built for SEO integrations.",
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://github.com/harlan-zw/nuxt-seo-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-seo-ui.git"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/nuxt-seo-ui/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --fix",
    "build": "nuxi prepare .playground && nuxt-module-build build",
    "dev": "nuxi dev .playground",
    "dev:build": "nuxi build .playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxi prepare .playground",
    "release": "bumpp package.json --commit --push --tag",
    "test": "true"
  },
  "dependencies": {
    "@nuxt/kit": "^3.7.4",
    "nuxt-icon": "^0.5.0",
    "nuxt-site-config": "^1.5.4",
    "nuxt-site-config-kit": "^1.5.4",
    "pathe": "^1.1.1",
    "radix3": "^1.1.0",
    "ufo": "^1.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.0.0-beta.27",
    "@nuxt/content": "^2.8.5",
    "@nuxt/module-builder": "^0.5.2",
    "@nuxt/test-utils": "3.7.4",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "bumpp": "^9.2.0",
    "eslint": "8.51.0",
    "execa": "^8.0.1",
    "nuxt": "^3.7.4",
    "vitest": "^0.34.6"
  }
}
